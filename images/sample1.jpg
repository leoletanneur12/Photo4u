<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 800 600" width="800" height="600">
  <defs>
    <linearGradient id="skyGradient" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#87CEEB;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#E0F6FF;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="groundGradient" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#90EE90;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#228B22;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Sky -->
  <rect width="800" height="400" fill="url(#skyGradient)"/>
  
  <!-- Sun -->
  <circle cx="650" cy="100" r="60" fill="#FFD700" opacity="0.8"/>
  
  <!-- Mountains -->
  <polygon points="0,300 200,150 400,300" fill="#8B7355" opacity="0.8"/>
  <polygon points="300,300 500,100 700,300" fill="#A0826D" opacity="0.8"/>
  <polygon points="600,300 800,180 800,300" fill="#8B7355" opacity="0.7"/>
  
  <!-- Ground -->
  <rect y="300" width="800" height="300" fill="url(#groundGradient)"/>
  
  <!-- Trees -->
  <g>
    <rect x="100" y="350" width="20" height="80" fill="#8B4513"/>
    <circle cx="110" cy="350" r="40" fill="#228B22"/>
  </g>
  <g>
    <rect x="500" y="330" width="25" height="100" fill="#8B4513"/>
    <circle cx="512" cy="330" r="50" fill="#32CD32"/>
  </g>
  
  <!-- Text -->
  <text x="400" y="550" font-family="Arial" font-size="48" font-weight="bold" text-anchor="middle" fill="white" opacity="0.9">Paysage</text>
</svg>